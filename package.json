{
  "name": "virtuozplay-workspace",
  "workspaces": [
    "front"
  ],
  "scripts": {
    "dev": "cd front && yarn run dev",
    "build": "cd front && yarn run build",
    "preview": "cd front && yarn run preview",
    "test:unit": "cd front && yarn run test:unit",
    "test:e2e": "cd front && yarn run test:e2e",
    "test:e2e:dev": "cd front && yarn run test:e2e:dev",
    "build-only": "cd front && yarn run build-only",
    "type-check": "cd front && yarn run type-check",
    "codegen": "cd front && yarn run codegen",
    "lint": "cd front && yarn run lint",
    "format": "prettier --ignore-path .gitignore --config .prettierrc -w ."
  },
  "repository": "github.com/VirtuozPlay/VirtuozPlay",
  "dependencies": {
    "chart.js": "^4.3.1",
    "front": "^0.0.1",
    "vue-chartjs": "^5.2.0"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
