{
  "name": "virtuozplay",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "run-p --print-label dev:vite dev:service-worker",
    "dev:vite": "vite --host",
    "dev:service-worker": "tsc --watch --project tsconfig.sw.json",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build && tsc --project tsconfig.sw.json",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "codegen": "graphql-codegen --config codegen.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "format": "cd .. && yarn run format"
  },
  "repository": "github.com/VirtuozPlay/VirtuozPlay",
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "latest-3",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@vue/apollo-components": "^4.0.0-beta.5",
    "@vue/apollo-option": "^4.0.0-beta.5",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-template-compiler": "^2.7.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.7",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@types/serviceworker": "^0.0.69",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "latest",
    "cypress": "^12.14.0",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.15.0",
    "glob": "^10.2.7",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "latest",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "latest",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "packageManager": "yarn@3.6.0"
}
