{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["*.js", "*.mjs", "*.cjs", "*.ts", "*.mts", "*.cts", "*.vue", "*.css"],
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
